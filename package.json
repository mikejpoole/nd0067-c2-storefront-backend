  "dependencies": {
    "@types/express": "^4.17.9",
    "@types/pg": "^7.14.7",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "db-migrate": "^0.11.12",
    "db-migrate-pg": "^1.2.2",
